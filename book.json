{
    "title": "Правда и мифы о Vision Zero",
    "description": "Правда и мифы о Vision Zero",
    "language": "ru-ru",
    "author": "НКО Пошли Поехали",
    "links": {
        "sidebar": {
            "Пошли Поехали": "https://cloudnative.to"
        }
    },
    "plugins": [

        "page-toc-button",
        "image-captions",
        "editlink",
        "back-to-top-button",
        "-lunr", "-search", "search-plus",
        "github-buttons@2.1.0",
        "favicon@^0.0.2",
        "tbfed-pagefooter@^0.0.1",
        "3-ba",
        "theme-default",
        "-highlight", "prism", "prism-themes",
        "lightbox",
        "ga",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "github": {
            "url": "https://github.com/NikiN2/VisionZero"
        },
        "editlink": {
            "base": "https://github.com/NikiN2/VisionZeroblob/master/",
            "label": "редактировать"
        },
        "image-captions": {
            "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_：_CAPTION_"
        },
        "github-buttons": {
            "repo": "NikiN2/VisionZero",
            "types": ["star"],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "</p>Copyright © 2021 | Distributed under <a href=https://creativecommons.org/licenses/by-nc-sa/4.0/deed.ru>CC BY 4.0</a> ",
            "modify_label": " Updated at ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "3-ba": {
            "token": "токен"
        },
        "prism": {
            "css": [
            "prism-themes/themes/prism-ghcolors.css"
            ]
        },
        "sitemap-general": {
            "prefix": "https://visionzero.рф/"
        },
        "ga": {
            "token": "токен"
        }
    }
}